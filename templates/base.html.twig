<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>

    <nav>
        <li><a href="{{ path('app_homepage') }}">Homepage</a></li>
        <li><a href="{{ path('app_recipes') }}">All Recipes</a></li>
        <li><a href="{{ path('app_recipes_show') }}">Recipe: Bake cookies</a></li>
        <li><a href="{{ path('app_skills') }}">All Skill Videos</a></li>
        <li><a href="{{ path('app_skills_show') }}">Skill Video: Sharpen Knives</a></li>
    </nav>

    <body>
        {% block body %}{% endblock %}
    </body>
</html>
